@inproceedings{tigwell_nuanced_2021,
	address = {Yokohama Japan},
	title = {Nuanced {Perspectives} {Toward} {Disability} {Simulations} from {Digital} {Designers}, {Blind}, {Low} {Vision}, and {Color} {Blind} {People}},
